@using MyEvernote.WebApp.ViewModels
@model OkViewModel
@{
    ViewBag.Title = Model.Title ;
}


<div class="container" style="margin-top:50px">
    <div class="row">
        <div class="col-md-4 col-md-offset-4">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h3 class="panel-title text-center">@Model.Header</h3>
                </div>
                <div class="panel-body">
                    <h2><span class="glyphicon glyphicon-ok-circle"></span>&nbsp;&nbsp;<strong>@Model.Title</strong></h2>
                    <hr />
                    <p class="text-justify">
                        @foreach (string msg in Model.Items)
                        {
                            @msg<br />
                        }
                    </p>

                </div>
            </div>
        </div>
    </div>
</div>

@{ 
    string js = string.Empty;
    if (Model.IsRedirecting)
    {
        js = "setTimeout(function(){window.location = '"+Model.RedirectingUrl+"';},"+Model.RedirectingTimeout+");";
    }
}

<script>
    $(function () {
         
     @Html.Raw(js);

    });
</script>



